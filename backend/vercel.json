{
  "version": 2,
  "name": "rose-bud-thorn-api",
  "builds": [
    {
      "src": "src/serverless.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/serverless.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/serverless.ts": {
      "maxDuration": 30
    }
  },
  "installCommand": "yarn install --frozen-lockfile",
  "buildCommand": "yarn build"
}